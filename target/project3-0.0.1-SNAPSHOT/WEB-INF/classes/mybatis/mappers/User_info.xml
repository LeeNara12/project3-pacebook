<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="User_infoDAO">

	<select id='login' parameterType='PaceUserVO' resultType='PaceUserVO'>
		select * 
		from user_info
		where user_id = #{id}
		and user_pw = #{pw} 
	</select>
	
	
</mapper>